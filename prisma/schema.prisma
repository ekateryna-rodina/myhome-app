// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Property {
  id      Int    @id @default(autoincrement())
  city    String
  country String
  title   String
  beds    Int
  baths   String
  size    Int
  photo   String
}
