generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Property {
  id      Int         @id @default(autoincrement())
  city    String
  country String
  zip     String      @default("")
  title   String
  beds    Int
  baths   String
  size    Int
  photo   String
  for     ServiceType @default(RENT)
}

enum ServiceType {
  RENT
  BUY
}
